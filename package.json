        "@types/js-yaml": "^3.12.0",
        "@types/node": "^10.9.4",
        "css-loader": "^1.0.0",
        "file-loader": "^3.0.1",
        "fork-ts-checker-webpack-plugin": "^0.5.0",
        "glob": "^7.1.3",
        "html-webpack-plugin": "^3.2.0",
        "node": "^10.10.0",
        "node-sass": "^4.13.1",
        "npm-run-all": "^4.1.3",
        "raw-loader": "^0.5.1",
        "sass-loader": "^7.0.3",
        "source-map-support": "^0.5.9",
        "style-loader": "^0.21.0",
        "ts-loader": "^4.4.2",
        "ts-node": "^7.0.0",
